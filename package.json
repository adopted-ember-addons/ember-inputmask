{
  "name": "ember-inputmask",
  "version": "0.11.0",
  "description": "Ember wrapper for Inputmask 4.x.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/brandynbennett/ember-inputmask",
  "license": "MIT",
  "author": "Chris Garrett",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "contributors": [
    {
      "name": "Brandyn Bennett"
    }
  ],
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "ember-auto-import": "^1.5.3",
    "ember-cli-babel": "^7.18.0",
    "ember-cli-htmlbars": "^4.2.3",
    "inputmask": "5.0.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.17.0",
    "ember-cli-addon-docs": "0.6.16",
    "ember-cli-addon-docs-yuidoc": "^0.2.4",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^7.0.0",
    "ember-set-helper": "^0.1.1",
    "ember-source": "~3.17.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.4.0",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-ember": "^8.1.1",
    "eslint-plugin-node": "^11.0.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.1.0"
  },
  "resolutions": {
    "js-yaml": "3.13.1",
    "lodash.template": "4.5.0",
    "cryptiles": "4.1.2",
    "mime": "1.4.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://brandynbennett.github.io/ember-inputmask/"
  },
  "homepage": "https://brandynbennett.github.io/ember-inputmask/"
}
